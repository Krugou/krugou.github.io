{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env PORT=3001 next dev --experimental-https",
    "build": "next build",
    "lint": "eslint --quiet",
    "lint:quiet": "eslint --quiet",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --quiet --reporter=list",
